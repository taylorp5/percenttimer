{
  "expo": {
    "name": "PercentTimer",
    "slug": "percenttimer",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/PTIOSICON.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-dev-client",
      [
        "./plugins/withSharedState",
        {
          "appGroupIdentifier": "group.com.taylor.percenttimer"
        }
      ],
      "@sentry/react-native"
    ],
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taylor.percenttimer",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/PTIOSICON.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.taylor.percenttimer",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d52d7537-84fd-4023-a690-825d89f72e8d"
      }
    }
  }
}
