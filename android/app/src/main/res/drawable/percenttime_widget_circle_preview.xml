<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="rectangle">
      <solid android:color="#22FFFFFF" />
      <stroke android:width="1dp" android:color="#1AFFFFFF" />
      <corners android:radius="16dp" />
    </shape>
  </item>
  <item android:width="72dp" android:height="72dp" android:gravity="center">
    <shape android:shape="ring" android:useLevel="false">
      <solid android:color="#1A000000" />
      <stroke android:width="6dp" android:color="#1A000000" />
    </shape>
  </item>
  <item android:width="72dp" android:height="72dp" android:gravity="center">
    <shape android:shape="ring" android:useLevel="false">
      <solid android:color="#3B82F6" />
      <stroke android:width="6dp" android:color="#3B82F6" />
    </shape>
  </item>
  <item android:gravity="center">
    <shape android:shape="rectangle">
      <size android:width="32dp" android:height="12dp" />
      <solid android:color="#111827" />
      <corners android:radius="3dp" />
    </shape>
  </item>
</layer-list>
